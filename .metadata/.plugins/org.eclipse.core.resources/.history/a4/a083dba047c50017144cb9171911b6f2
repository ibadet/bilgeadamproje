package com.fazlaysapaylas.domain;

public class Urun {
	
	private Long id;
	private String tanim;
	private Kategori kategori;
	private Marka marka;
	private String kullanýmDurumu;
	private String aciklama;
	private Integer puan; 
	private byte[] fotograf;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getTanim() {
		return tanim;
	}
	public void setTanim(String tanim) {
		this.tanim = tanim;
	}
	public Kategori getKategori() {
		return kategori;
	}
	public void setKategori(Kategori kategori) {
		this.kategori = kategori;
	}
	public Marka getMarka() {
		return marka;
	}
	public void setMarka(Marka marka) {
		this.marka = marka;
	}
	public String getKullanýmDurumu() {
		return kullanýmDurumu;
	}
	public void setKullanýmDurumu(String kullanýmDurumu) {
		this.kullanýmDurumu = kullanýmDurumu;
	}
	public String getAciklama() {
		return aciklama;
	}
	public void setAciklama(String aciklama) {
		this.aciklama = aciklama;
	}
	public Integer getPuan() {
		return puan;
	}
	public void setPuan(Integer puan) {
		this.puan = puan;
	}
	public byte[] getFotograf() {
		return fotograf;
	}
	public void setFotograf(byte[] fotograf) {
		this.fotograf = fotograf;
	}
	
	
	
	

}
